<div class="container">
    <div class="row">
    <div class="col-12 general-content">
    <isif condition="${!empty(block.title)}">
        <h2 class="content-header" ${cmsUtils.cslp(block, 'title')}>${block.title}</h2>
    </isif>
    <isif condition="${!cmsUtils.isJsonRteEmpty(block.copy)}">
        <div ${cmsUtils.cslp(block, 'copy')}>
            <isprint value="${cmsHelper.rteToHtml(block.copy)}" encoding="off" />
        </div>
    </isif>
</div>
</div>
</div>